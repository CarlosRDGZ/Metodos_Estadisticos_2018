TukeyHSD(modelo)
par(mar=c(6,11,3,1))
plot(TukeyHSD(modelo,'Trt_Soluciones'), las=1, col="brown")
library(agricolae)
Prueba <- HSD.test(modelo, "Trt_Soluciones", group=TRUE)
Prueba$groups
qqnorm(modelo$residuals)
qqline(modelo$residuals)
shapiro.test(modelo$residuals)
